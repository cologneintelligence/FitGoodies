		<tag>HEAD</tag>
		<url>http://sourceforge.net/apps/trac/fitgoodies/browser/trunk</url>
	</scm>
	<modules>
		<module>fitgoodies</module>
		<module>archetype-fitgoodies-quickstart</module>
